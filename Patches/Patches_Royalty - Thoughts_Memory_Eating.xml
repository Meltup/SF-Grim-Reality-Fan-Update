<Patch>

	<!-- AteFoodInappropriateForTitle -->

	<Operation Class="PatchOperationReplace">
		<xpath>*/ThoughtDef[defName = "AteFoodInappropriateForTitle"]/durationDays</xpath>
		<value>
			<durationDays>0.5</durationDays>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>*/ThoughtDef[defName = "AteFoodInappropriateForTitle"]</xpath>
		<value>
			<stackLimit>1</stackLimit>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>*/ThoughtDef[defName = "AteFoodInappropriateForTitle"]/stages/li[1]/baseMoodEffect</xpath>
		<value>
			<baseMoodEffect>-5</baseMoodEffect>
		</value>
	</Operation>

</Patch>
